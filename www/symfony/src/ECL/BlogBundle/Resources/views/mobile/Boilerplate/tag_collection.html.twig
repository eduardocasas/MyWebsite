<ul id="tag_boilerplate_list">
{% for item in tags %}
    <li>
    {% if blog_tag_selected != null and blog_tag_selected ==  item.slug %}
        <strong>{{ item.name }}</strong>
    {% else %}
        <a href="{{ url('ecl_blog_homepage', { tag_slug: item.slug } ) }}">{{ item.name }}</a>
    {% endif %}
    </li>
{% endfor %}
</ul>